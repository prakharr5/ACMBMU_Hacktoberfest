//#include <iostream>
using namespace std;

int main()
{
   int n = 5;
    for(int i=1;i<=n;i++) {
    for(int j=1;j<=n;j++) {
        cout<<"*   ";
        }
    cout<<"\n";
    }
     return 0;
}//
#include <iostream>
using namespace std;

int main()
{
   int n = 5;
    for(int i=1;i<=n;i++) {
    for(int j=1;j<=i;j++) {
        cout<<"*   ";
        }
    cout<<"\n";
    }
     return 0;
}
